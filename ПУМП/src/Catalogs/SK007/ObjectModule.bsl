Процедура ПередЗаписью(Отказ)
	пНовый=ЭтотОбъект.ЭтоНовый();
	пКод=СОКРЛП(ЭтотОбъект.NUMBER)+"-"+СОКРЛП(ЭтотОбъект.LPU)+"-"+СОКРЛП(ЭтотОбъект.LPU_PODR)+"-"+
    	    		ОбщегоНазначенияПГБ2.DTOS(ЭтотОбъект.DATE_BEG);
	ЭтотОбъект.Код=пКод;
	Если пНовый Тогда
		ЭтотОбъект.ДатаСоздания=ТекущаяДата();
		ЭтотОбъект.ДатаИзменения=ЭтотОбъект.ДатаСоздания;
